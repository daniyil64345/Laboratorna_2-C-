#include <iostream>
#include <cmath>

int main()
{
	double x, y;
	bool res_x, res_y;
	
	std::cout << "Coordinate x = ";
	std::cin >> x;

	std::cout << "Coordinate y = ";
	std::cin >> y;

	if (x > 0 && x < 2) {
		res_x = true;
	}
	else {
		res_x = false;
	}

	if (y > 0 && y < 1) {
		res_y = true;
	}
	else {
		res_y = false;
	}

	if (res_x && res_y) {
		std::cout << "All coordinate is great\n";
	}
	if (res_x && !res_y) {
		std::cout << "Y is not great\n";
	}
	if (!res_x && res_y) {
		std::cout << "X is not great\n";
	}
	if (!res_x && !res_y) {
		std::cout << "Everything went wrong\n";
	}
}
